{
  "manifest_version": 3,
  "name": "DeepKeys - Shortcuts for DeepSeek",
  "version": "1.1",
  "host_permissions": ["https://chat.deepseek.com/*"],
  "content_scripts": [
    {
      "matches": ["https://chat.deepseek.com/*"],
      "js": ["content.js"]
    }
  ],
  "action": {
    "default_popup": "popup/popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  }
}